<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" 
  package="net.ramptors.aarchyfor">
  <uses-permission android:name="android.permission.INTERNET" />
  <application
    android:label="@string/app_name" 
    android:theme="@style/AppTheme">
    <activity android:name=".CtrlUsuarios" 
      android:label="@string/usuarios">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".CtrlUsuarioNuevo" 
      android:label="@string/usuario_nuevo" 
      android:parentActivityName=".CtrlUsuarios">
      <meta-data android:name="android.support.PARENT_ACTIVITY" 
        android:value=".CtrlUsuarios" />
    </activity>
    <activity android:name=".CtrlUsuario" 
      android:label="@string/usuario" 
      android:parentActivityName=".CtrlUsuarios">
      <meta-data android:name="android.support.PARENT_ACTIVITY" 
        android:value=".CtrlUsuarios" />
    </activity>
  </application>
</manifest>